{
  "manifest_version": 2,

  "name": "Kitty Focus",
  "description": "Collect kitties by being active on sites that you mark as productive.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
