{
  "manifest_version": 2,

  "name": "Kitty Focus",
  "description": "Collect kitties by being active on sites that you mark as productive.",
  "version": "1.0",
  "options_page": "options.html",

  "background" : {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
